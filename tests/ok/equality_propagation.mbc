name n : index->message

system null.

goal eq_timestamps :

  forall (tau1:timestamp, tau2:timestamp)
  such that tau1 = tau2,
  output@tau1 = output@tau2.

Proof.
Qed.

goal eq_names :

  forall (i:index, j:index)
  such that i<>j,
  n(i) <> n(j).

Proof.
Qed.

goal functionality :
  forall (x:message, y:message),
  x = y => fst(x) = fst(y).
Proof.
Qed.

goal contradiction :
  forall (x:message, y:message),
  (x = y && x <> y) => false.
Proof.
Qed.
