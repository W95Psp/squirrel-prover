hash h
name k:message
name cst:message

channel c

name na : index -> message
name nb : index -> message
name nc : index -> message
name mc : index -> message

axiom name_not_pair :
forall (ma : message, mb : message, a:index),
na(a) <>  <ma, mb>

system (out(c,cst);
 (
  (!_a out(c,h(na(a),k)) as A)
  | (!_b out(c,h(<nb(b),nb(b)>,k)) as B)
  | (!_c out(c,h(<nc(c),mc(c)>,k)) as C)
 )) as O.


goal dummy :
  forall (tau1 : timestamp, tau2 : timestamp, a : index, a: message),
  tau1 = tau2 =>
  output@tau1= output@tau2.
Proof.
Qed.

goal unforgeable_1 :
  forall (a : index, b : index),
  b <> a =>
  output@A(b) <> h(na(a),k).

Proof.
  collision.
Qed.

goal unforgeable_2 :
  forall (a : index, b : index),
  output@B(b) <> h(na(a),k).

Proof.
  collision.
  apply name_not_pair to
  nb(b),
  nb(b),
  a.
Qed.


goal unforgeable_3 :
  forall (a : index, b : index),
  output@C(b) <> h(na(a),k).

Proof.
  collision.

  apply name_not_pair to
  nc(b),
  mc(b),
  a.
Qed.
