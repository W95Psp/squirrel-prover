abstract ok : message
abstract ko : message

abstract f : message->message

name n : message
name m : message
name k : message

system null.

equiv testXorOneArg : diff(f(ok),f(ok)),diff(n,m) XOR k.

Proof.
  xor 1.
Qed.

equiv testXorTwoArg : diff(f(ok),f(ok)),diff(ko,ko) XOR diff(n,m) XOR k.

Proof.
  xor 1, k.
Qed.
